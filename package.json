{
  "name": "valorant-vue-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "scraper": "tsx --tsconfig tsconfig.scraper.json src/scraper/index.ts",
    "scheduler": "tsx --tsconfig tsconfig.scraper.json src/scraper/scheduler.ts",
    "verify-data": "tsx --tsconfig tsconfig.scraper.json src/scraper/verify-data.ts",
    "api": "tsx --tsconfig tsconfig.scraper.json src/server/api.ts",
    "start": "concurrently \"npm run api\" \"npm run dev\""
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "mongodb": "^6.3.0",
    "node-cron": "^3.0.3",
    "pinia": "^3.0.1",
    "ts-node": "^10.9.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.22",
    "@types/node": "^22.14.0",
    "@types/node-cron": "^3.0.11",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "tsx": "^4.7.0",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
